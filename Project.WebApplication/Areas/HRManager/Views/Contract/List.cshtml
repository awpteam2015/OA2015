@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
}

@section JavaScript{
    <script src="/Areas/HRManager/Scripts/Contract/List.js?JsParamter=@SiteConfig.GetConfig().JsParamter" type="text/javascript"></script>
}

<div title="合同列表" style="padding:1px;">
    <table id="datagrid" style="height:auto;" iconcls=".icon-save" singleselect="true" toolbar="#tb" fit="true"></table>
    <div id="tb" style="padding:5px;height:auto">
        <div style="margin-bottom: 5px">
            <a id="btnAdd" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true">添加新合同</a>

            <a id="btnAdd2" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true">续订合同</a>

            <a id="btnAdd3" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true">变更合同</a>

            <a id="btnAdd4" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true">终止合同</a>

            <a id="btnDel" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-remove',plain:true">删除</a>

            <a id="btnEdit" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true">编辑</a>

            <a id="btnUpload" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true">批量上传合同</a>
            <a id="btnView" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true">查看</a>

            <a id="btnRefresh" href="javascript:void(0)" class="easyui-linkbutton" data-options="iconCls:'icon-reload',plain:true">刷新</a>

            <a href="~/TemplateFile/合同上传模版.xlsx"  class="easyui-linkbutton" data-options="iconCls:'icon-print',plain:true">模版下载</a>
        </div>

        <div id="divSearch">
            <form>

                <table class="mtable">

                    <tr>
                        <td>
                            工号:
                        </td>
                        <td>
                            <input id="EmployeeCode" name="EmployeeCode" type="text" />
                        </td>

                        <td>
                            部门编号:
                        </td>
                        <td>
                            <input id="DepartmentCode" name="DepartmentCode" type="text" style="width: 300px" />
                        </td>

                        <td>
                            开始时间:
                        </td>
                        <td>
                            <input id="BeginDate" name="BeginDate" type="text" onclick="WdatePicker();" />
                        </td>

                        <td>
                            结束时间:
                        </td>
                        <td>
                            <input id="EndDate" name="EndDate" type="text" onclick="WdatePicker();" />
                        </td>
                    </tr>





                    <tr>
                        <td>
                            合同编号:
                        </td>
                        <td>
                            <input id="ContractNo" name="ContractNo" type="text" />
                        </td>
                        <td>
                            身份证:
                        </td>
                        <td>
                            <input id="IdentityCardNo" name="IdentityCardNo" type="text" style="width: 300px" />
                        </td>
                        <td>
                            甲方:
                        </td>
                        <td>
                            <input id="FirstParty" name="FirstParty" type="text" />
                        </td>

                        <td>
                            已方:
                        </td>
                        <td>
                            <input id="SecondParty" name="SecondParty" type="text" />
                        </td>


                    </tr>

                    <tr>
                        <td>
                            合同状态：
                        </td>
                        <td>

                            <select id="State" name="State">
                                <option value="0">全部</option>
                                <option value="1">新签</option>
                                <option value="2">续签</option>
                                <option value="3">变更</option>
                                <option value="4">终止</option>
                            </select>
                        </td>

                        <td>
                            是否有效:
                        </td>
                        <td>
                            <select id="IsActive" name="IsActive">
                                <option value="0">全部</option>
                                <option value="1">有效</option>
                                <option value="2">无效</option>
                            </select>
                        </td>


                        <td colspan="2">
                            <a href="#" class="easyui-linkbutton" iconcls="icon-search" id="btnSearch">搜索</a>
                        </td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
</div>





