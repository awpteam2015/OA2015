@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_LayoutList.cshtml";
}
@section JavaScript{
    <script src="~/Areas/ReportManager/Scripts/HrReport/EmployeeInOutReport.js"></script>
}
<div title="员工出入统计报表" style="padding:1px;">
    <table id="datagrid" style="height:auto;" iconcls=".icon-save" singleselect="true" toolbar="#tb" fit="true"></table>
    <div id="tb" style="padding:5px;height:auto">

        <div id="divSearch">
            <form>
                <table class="mtable">
                    <tr>
                        <td style="width: 100px">
                             类型:
                        </td>
                        <td style="width: 100px">
                            <select id="InOrOut" name="InOrOut" class="easyui-combobox">
                                <option value="-1">全部</option>
                                <option value="0">进</option>
                                <option value="1">出</option>
                            </select>
                        </td>
                        <td style="width: 100px">
                            部门:
                        </td>
                        <td style="width: 100px">
                            <input id="DepartmentCode" name="DepartmentCode" type="text" />
                        </td>
                        <td style="width: 100px">
                            开始时间:
                        </td>
                        <td style="width: 100px">
                            <input id="Date" name="Date" type="text" class="Wdate" onclick="WdatePicker();" />
                        </td>
                        <td style="width: 100px">
                            至:
                        </td>
                        <td style="width: 100px">
                            <input id="EndDate" name="EndDate" type="text" class="Wdate" onclick="WdatePicker();" />
                        </td>
                        <td>
                            <a href="#" class="easyui-linkbutton" iconcls="icon-search" id="btnSearch">搜索</a>
                            <a href="#" class="easyui-linkbutton" iconcls="icon-save" id="btnExport">导出</a>
                        </td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
</div>


